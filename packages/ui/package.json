{
  "name": "@mint/ui",
  "author": "Minimals",
  "version": "7.0.0",
  "description": "Next & TypeScript",
  "private": true,
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "type": "module",
  "scripts": {
    "next:build": "next build",
    "next:dev": "next dev -p 8082 --turbopack",
    "next:start": "next start -p 8082",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist next:build",
    "re:dev": "yarn clean && yarn install && yarn next:dev",
    "re:build": "yarn clean && yarn install && yarn next:build",
    "re:build-npm": "npm run clean && npm install && npm run next:build",
    "tsc:dev": "yarn next:dev & yarn tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:print": "npx tsc --showConfig"
  },
  "engines": {
    "node": ">=22"
  },
  "turbo": {
    "tags": [
      "internal",
      "no-build"
    ]
  },
